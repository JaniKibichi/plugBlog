<h1 id="page_title">What we are working on</h1>
 <div id="posts_wrapper" class ="skinny_wrapper">
  <% @projects.each do |project| %>

    <div class="post">
      <p class="date"><%= project.created_at.strftime("%A, %b %d") %></p>
      <h2><a href="#"><%= link_to project.title, user_project_path(current_user, project) %></a></h2>
      <hr>
    </div>

  <% end %>
 </div>

